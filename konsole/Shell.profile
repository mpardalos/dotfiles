[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=Breeze
Font=Fira Code,11,-1,5,50,0,0,0,0,0

[Cursor Options]
CursorShape=2

[General]
Command=/bin/zsh
LocalTabTitleFormat=
Name=Shell
Parent=FALLBACK/
RemoteTabTitleFormat=%u@%H
ShowTerminalSizeHint=false
