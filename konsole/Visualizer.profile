[Appearance]
BoldIntense=false
ColorScheme=BlueOnBlack
Font=Fira Mono,12,-1,5,50,0,0,0,0,0

[Cursor Options]
CursorShape=2

[General]
Command=/bin/zsh
Environment=TERM=xterm-256color,COLORTERM=truecolor
LocalTabTitleFormat=cava_widget
Name=Visualizer
Parent=FALLBACK/
RemoteTabTitleFormat=%u@%H
ShowTerminalSizeHint=false
TerminalColumns=160
TerminalRows=25

[Scrolling]
HistoryMode=0
ScrollBarPosition=2

[Terminal Features]
BlinkingCursorEnabled=false
